{
    "name": "max/swoole-api-framework",
    "description": "Высокопроизводительный php-фреймворк на основе Swoole.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SwooleAPI\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "ext-swoole": ">=4.8.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "ext-openssl": "Required for HTTPS support"
    }
}
